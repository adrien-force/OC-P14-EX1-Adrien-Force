<nav aria-label="Pagination" class="d-flex justify-content-center" {{ attributes }}>
    <ul class="pagination">
        {% for page in pagination %}
            {% if page.active %}
                <li class="page-item active" aria-current="true">
                    <span class="page-link">{{ page.label }}</span>
                </li>
            {% else %}
                <li class="page-item">
                    <a class="page-link" href="{{ page.url }}">{{ page.label }}</a>
                </li>
            {% endif %}
        {% endfor %}
    </ul>
</nav>
